{
  "version": 2,
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/Cube.svg",
      "destination": "/public/Cube.svg"
    },
    {
      "source": "/cube2.svg",
      "destination": "/public/cube2.svg"
    },
    {
      "source": "/(Mesa|Turkey|Cooledge|Home|commercial|contact|Residential)/(.*)",
      "destination": "/public/$1/$2"
    },
    {
      "source": "/(Bureau of Land Management|Central Commercial Building Renovation|College Condos and Stadium Condos|Mohave Mental Health|navigation bar)/(.*)",
      "destination": "/public/$1/$2"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}

